{
	"semi": true,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 1,
	"trailingComma": "es5",
	"printWidth": 100,
	"arrowParens": "always",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["^react$", "^ink$", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
